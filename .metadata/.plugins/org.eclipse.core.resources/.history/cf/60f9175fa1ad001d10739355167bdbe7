package com.codingdojo.fruityloops.models;


public class Items {
    // MEMBER VARIABLES
    private String name;
    private double price;
    // CONSTRUCTOR
    public Items(String name, double price) {
        this.name = name;
        this.price = price;
    }
    
    // As always, don't forget to generate Getters and Setters!
}
